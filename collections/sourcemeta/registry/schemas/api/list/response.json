{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [ "health", "entries", "url", "canonical", "breadcrumb" ],
  "properties": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "breadcrumb": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [ "name", "url" ],
        "properties": {
          "name": {
            "type": "string"
          },
          "url": {
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    "canonical": {
      "type": "string",
      "format": "uri"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "entries": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [ "health", "name", "type", "url" ],
        "properties": {
          "title": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "email": {
            "type": "string",
            "format": "email"
          },
          "github": {
            "type": "string"
          },
          "health": {
            "type": "number",
            "maximum": 100,
            "minimum": 0
          },
          "name": {
            "type": "string"
          },
          "url": {
            "type": "string"
          },
          "website": {
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    "github": {
      "type": "string"
    },
    "health": {
      "type": "number",
      "maximum": 100,
      "minimum": 0
    },
    "url": {
      "type": "string"
    },
    "website": {
      "type": "string",
      "format": "uri"
    }
  },
  "additionalProperties": false
}

{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "bytes",
    "id",
    "url",
    "canonical",
    "baseDialect",
    "dialect",
    "health",
    "breadcrumb"
  ],
  "properties": {
    "id": {
      "type": "string",
      "format": "uri"
    },
    "baseDialect": {
      "type": "string"
    },
    "breadcrumb": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [ "name", "url" ],
        "properties": {
          "name": {
            "type": "string"
          },
          "url": {
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    },
    "bytes": {
      "type": "number",
      "minimum": 0
    },
    "canonical": {
      "type": "string",
      "format": "uri"
    },
    "dialect": {
      "type": "string",
      "format": "uri"
    },
    "health": {
      "$ref": "../common/percentage.json"
    },
    "url": {
      "type": "string"
    }
  },
  "additionalProperties": false
}

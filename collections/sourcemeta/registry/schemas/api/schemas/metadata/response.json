{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$ref": "#base",
  "required": [ "breadcrumb" ],
  "properties": {
    "breadcrumb": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [ "name", "path" ],
        "properties": {
          "name": {
            "type": "string"
          },
          "path": {
            "$ref": "../../../../../std/v0.0.1/schemas/uri-relative.json"
          }
        },
        "additionalProperties": false
      }
    }
  },
  "unevaluatedProperties": false,
  "$defs": {
    "base": {
      "$anchor": "base",
      "type": "object",
      "required": [
        "bytes",
        "identifier",
        "path",
        "baseDialect",
        "dialect",
        "protected",
        "alert",
        "health"
      ],
      "properties": {
        "title": {
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "alert": {
          "type": [ "string", "null" ]
        },
        "baseDialect": {
          "$ref": "../../../../../std/v0.0.1/schemas/uri.json"
        },
        "bytes": {
          "$ref": "../../../../../std/v0.0.1/schemas/bytes.json"
        },
        "dialect": {
          "$ref": "../../../../../std/v0.0.1/schemas/uri.json"
        },
        "health": {
          "$ref": "../../../../../std/v0.0.1/schemas/percentage.json"
        },
        "identifier": {
          "$ref": "../../../../../std/v0.0.1/schemas/url.json"
        },
        "path": {
          "$ref": "../../../../../std/v0.0.1/schemas/uri-relative.json"
        },
        "protected": {
          "type": "boolean"
        }
      }
    }
  }
}

{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "array",
  "items": {
    "type": "object",
    "required": [ "from", "to", "at" ],
    "properties": {
      "at": {
        "type": "string",
        "format": "json-pointer"
      },
      "from": {
        "type": "string",
        "format": "uri"
      },
      "to": {
        "type": "string",
        "format": "uri"
      }
    },
    "additionalProperties": false
  }
}
